<h1>Listing campaigns</h1>

<%= link_to 'csv', landing_cat.admin_campaigns_path( :format => :csv ) %>
<br/><br/>

<table border="1">
  <thead>
    <tr>
      <th>id</th>
      <th>utmcsr</th>
      <th>utmcmd</th>
      <th>utmctr</th>
      <th>utmcct</th>
      <th>utmccn</th>
      <th>created_at</th>
    </tr>
  </thead>

  <tbody>
    <% @campaigns.each do |campaign| %>
      <tr>
        <td><%= campaign.id %></td>
        <td><%= campaign.utmcsr %></td>
        <td><%= campaign.utmcmd %></td>
        <td><%= campaign.utmctr %></td>
        <td><%= campaign.utmcct %></td>
        <td><%= campaign.utmccn %></td>
        <td><%= campaign.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @campaigns %>

